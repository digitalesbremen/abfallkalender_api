# Read all streets of Bremen
GET {{baseUrl}}/abfallkalender-api/streets
Accept: application/json

###

# Read specific street
GET {{baseUrl}}/abfallkalender-api/street/Aachener+Straße
Accept: application/json

###

# Read ICS format
GET {{baseUrl}}/abfallkalender-api/street/Aachener+Straße/number/22
Accept: text/calendar

###

# Read CSV format
GET {{baseUrl}}/abfallkalender-api/street/Aachener+Straße/number/22
Accept: text/csv

###

# Read next
GET {{baseUrl}}/abfallkalender-api/street/Aachener+Straße/number/22/next

###